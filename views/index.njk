{% set title = "Strava" %}
{% extends "html.njk" %}

{% block body %}
<pre>{{ data | dump }}</pre>
<dl>
{% for key in data %}
<dt>{{ key }}</dt>
<dd>{{ data[key] }}</dt>
{% endfor %}
</dl>

<form method="POST" action="/deauthorize">
<input type="submit" value="Deauthorize" />
</form>
</form>
{% endblock %}